         Spacecraft.cpp UTCTime.cpp
      ;

   InstallFile $(INCDIR) :
         ASConstant.hpp EarthBody.hpp EarthOceanTide.hpp EarthPoleTide.hpp EarthSolidTide.hpp EquationOfMotion.hpp ForceModel.hpp ForceModelList.hpp 
         IERS.hpp Integrator.hpp 
         KeplerOrbit.hpp ReferenceFrames.hpp RungeKuttaFehlberg.hpp
         Spacecraft.hpp UTCTime.hpp
      ;
}
 