    ObsReader.cpp EphReader.cpp MetReader.cpp
    ;

  InstallFile $(INCDIR) : MDPHeader.hpp MDPPVTSolution.hpp MDPNavSubframe.hpp
    MDPObsEpoch.hpp MDPSelftestStatus.hpp RinexConverters.hpp DataStatus.hpp
    miscenum.hpp FDStreamBuff.hpp  TCPStreamBuff.hpp TCPStream.hpp
    DeviceStream.hpp
    ObsUtils.hpp FFIdentifier.hpp
    ObsReader.hpp EphReader.hpp MetReader.hpp
    ;
}