{
   GPSSetupLibrary rxio : gpstk ;
}
else
{
  GPSBuildLibrary rxio : MDPHeader.cpp MDPPVTSolution.cpp
    MDPNavSubframe.cpp MDPObsEpoch.cpp MDPSelftestStatus.cpp
    RinexConverters.cpp FDStreamBuff.cpp TCPStreamBuff.cpp
    DeviceStream.cpp
