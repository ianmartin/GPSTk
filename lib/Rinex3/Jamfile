SubDir TOP lib Rinex3 ;

if $(PREPROCESSING)
{
   GPSSetupLibrary Rinex3 : gpstk ;
}
else
{
  # Please add in alphabetic order
  GPSBuildLibrary Rinex3 : 
  GPSEphemerisStore.cpp  SP3EphemerisStore.cpp  TabularEphemerisStore.cpp
    ;

  InstallFile $(INCDIR) : 
  GPSEphemerisStore.hpp  KeplerianEphemerisStore.hpp  SP3EphemerisStore.hpp  TabularEphemerisStore.hpp
    ;
}
