   UnixTime.cpp
   YDSTime.cpp
   ../../src/SP3SatID.cpp ../../src/Triple.cpp ../../src/ECEF.cpp
   ../../src/Geodetic.cpp ../../src/TimeConverters.cpp
   ;
   InstallFile $(INCDIR) :
   ;
}

GPSLinkLibraries ttime : Rinex3 ;
GPSMain ttime : ttime.cpp ;
